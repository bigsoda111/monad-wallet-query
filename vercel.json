{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "PORT": "3000",
    "MONGODB_URI": "mongodb://localhost:27017/monad_wallets",
    "RPC_URL": "https://testnet-rpc.monad.xyz"
  }
} 